{% if settings.home_page_content != empty %}
  {% unless settings.home_page_full_width %}
  <div class="grid">
    <div class="
    grid__item
    large--one-half
    medium--two-thirds ">
  {% endunless %}
      {% if settings.home_show_headings and settings.home_page_show_heading %}
      <div class="section-header section-header--small">
        <h2 class="section-header__title">{{ pages[settings.home_page_content].title }}</h2>
      </div>
      {% endif %}
      <div class="rte">
        {{ pages[settings.home_page_content].content }}
      </div>

    </div>
    <div class="
    grid__item
    large--three-quarters
    medium--two-thirds ">
      <div class="section-header section-header--small">
        <h2 class="section-header__title">{{ pages[settings.home_page_content].title }}
        </h2>
      </div>
    </div>
    <div class="
    grid__item
    large--one-quarter
    medium--one-third
    ">
          <p class="grid-demo">2.3</p>
    </div>
    {% unless settings.home_page_full_width %}
  </div>
  {% endunless %}
{% endif %}
