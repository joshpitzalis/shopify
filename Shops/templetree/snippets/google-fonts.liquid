<script>window.fonts = [];</script>
{% if settings.type_header_family contains 'Google' %}
  {% unless settings.type_header_family contains type_base_parts[1] %}
    {% assign type_header_parts = settings.type_header_family | split: '_' %}
    {% capture headerGoogleUrl %}{{ type_header_parts[1] }}:{{ type_header_parts[2] }}{% endcapture %}
    <script>window.fonts.push('{{ headerGoogleUrl }}');</script>
    <noscript>{{ '//fonts.googleapis.com/css?family=' | append: headerGoogleUrl | stylesheet_tag }}</noscript>
  {% endunless %}
{% endif %}
{% if settings.type_accent_family contains 'Google' %}
  {% unless settings.type_accent_family contains type_base_parts[1] %}
    {% assign type_accent_parts = settings.type_accent_family | split: '_' %}
    {% capture accentGoogleUrl %}{{ type_accent_parts[1] }}:{{ type_accent_parts[2] }}{% endcapture %}
    <script>window.fonts.push('{{ accentGoogleUrl }}');</script>
    <noscript>{{ '//fonts.googleapis.com/css?family=' | append: accentGoogleUrl | stylesheet_tag }}</noscript>
  {% endunless %}
{% endif %}
{% if settings.type_buttons_family contains 'Google' %}
  {% unless settings.type_buttons_family contains type_base_parts[1] %}
    {% assign type_buttons_parts = settings.type_buttons_family | split: '_' %}
    {% capture buttonsGoogleUrl %}{{ type_buttons_parts[1] }}:{{ type_buttons_parts[2] }}{% endcapture %}
    <script>window.fonts.push('{{ buttonsGoogleUrl }}');</script>
    <noscript>{{ '//fonts.googleapis.com/css?family=' | append: buttonsGoogleUrl | stylesheet_tag }}</noscript>
  {% endunless %}
{% endif %}
{% if settings.type_base_family contains 'Google' %}
  {% assign type_base_parts = settings.type_base_family | split: '_' %}
  {% capture baseGoogleUrl %}{{ type_base_parts[1] }}:{{ type_base_parts[2] }}{% endcapture %}
  <script>window.fonts.push('{{ baseGoogleUrl }}');</script>
  <noscript>{{ '//fonts.googleapis.com/css?family=' | append: baseGoogleUrl | stylesheet_tag }}</noscript>
{% endif %}

